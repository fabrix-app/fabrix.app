{
  "name": "@fabrix/fabrix.app",
  "version": "1.0.0",
  "description": "Fabrix.app built on Fabrix",
  "homepage": "https://fabrix.app",
  "author": {
    "name": "scott-wyatt",
    "email": "scottwyatt86@gmail.com",
    "url": "https://cali-style.com"
  },
  "license": "MIT",
  "repository": "https://github.com/fabrix-app/fabrix.app.git",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "run-s prebuild build:client build:aot build:server",
    "build:prod": "run-s prebuild build:client:prod build:aot:prod build:server",
    "build:staging": "run-s prebuild build:client:staging build:aot:staging build:server:staging",
    "build:testing": "run-s prebuild build:client:testing build:aot:testing build:server:testing",
    "build:client": "ng build",
    "build:client:prod": "ng build --prod --build-optimizer",
    "build:client:staging": "ng build --environment=staging",
    "build:client:testing": "ng build --environment=testing",
    "build:aot": "ng build --aot && ng run fabrix:server",
    "build:aot:prod": "ng build --prod && ng run fabrix:server",
    "build:aot:staging": "ng build --aot --target=staging --environment=staging && ng run fabrix:server",
    "build:aot:testing": "ng build --aot --target=testing --environment=testing && ng run fabrix:server",
    "build:server": "webpack",
    "build:server:staging": "webpack",
    "build:server:testing": "webpack",
    "build:server:prod": "webpack -p",
    "build:prod:ngsw": "npm run build:prod && npm run ngsw:config && npm run ngsw:copy",
    "serve:prod": "npm run build:prod && node dist/server",
    "serve:prod:ngsw": "npm run build:prod:ngsw && node dist/server",
    "ngsw:config": "node_modules/.bin/ngsw-config dist src/ngsw-config.json",
    "ngsw:copy": "cp node_modules/@angular/service-worker/ngsw-worker.js dist/",
    "test": "run-s test:ng test:e2e test:server",
    "test:ng": "ng test --watch=false",
    "test:e2e": "ng e2e",
    "test:ng:watch": "ng test",
    "test:server": "npm run build && mocha",
    "lint": "ng lint --fix .",
    "e2e": "ng e2e",
    "heroku-postbuild": "npm run build:prod:ngsw",
    "prebuild": "node ./prebuild.js"
  },
  "main": "./dist/server.js",
  "dependencies": {
    "@angular/animations": "6.0.7",
    "@angular/cdk": "^6.3.2",
    "@angular/common": "6.0.7",
    "@angular/compiler": "6.0.7",
    "@angular/core": "6.0.7",
    "@angular/forms": "6.0.7",
    "@angular/http": "6.0.7",
    "@angular/material": "^6.3.2",
    "@angular/platform-browser": "6.0.7",
    "@angular/platform-browser-dynamic": "6.0.7",
    "@angular/platform-server": "6.0.7",
    "@angular/router": "6.0.7",
    "@angular/service-worker": "6.0.7",
    "@fabrix/fabrix": "^1.0.3",
    "@fabrix/spool-express": "^1.0.0",
    "@fabrix/spool-i18n": "^1.0.0",
    "@fabrix/spool-repl": "^1.0.0",
    "@fabrix/spool-router": "^1.0.0",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/entity": "^6.0.1",
    "@ngrx/router-store": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@swimlane/ngx-charts": "^8.1.0",
    "angular2-moment": "^1.9.0",
    "bootstrap": "^4.1.0",
    "core-js": "^2.4.1",
    "express": "^4.16.2",
    "hammerjs": "^2.0.8",
    "ng-engine": "^6.0.0",
    "ngrx-forms": "^2.3.6",
    "ngx-bootstrap": "^2.0.3",
    "ngx-json-viewer": "^2.3.0",
    "ngx-moment": "^2.0.0",
    "pluralize": "^7.0.0",
    "rxjs": "^6.2.1",
    "winston": "^2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.7",
    "@angular/language-service": "6.0.7",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/jest": "^20.0.2",
    "@types/node": "~9.4.7",
    "@types/winston": "^2.2.20-alpha",
    "codelyzer": "^4.0.1",
    "jasmine": "^2.5.3",
    "jasmine-core": "~2.8.0",
    "jasmine-marbles": "^0.3.1",
    "jasmine-spec-reporter": "~3.2.0",
    "jest": "^21.0.2",
    "jest-preset-angular": "^3.0.1",
    "jest-zone-patch": "^0.0.7",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^3.0.2",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.1.2",
    "rxjs-compat": "^6.2.1",
    "supertest": "^3.0.0",
    "ts-loader": "^3.1.1",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.7.2",
    "webpack-cli": "^3.0.8",
    "webpack-node-externals": "^1.6.0"
  },
  "engines": {
    "node": ">= 7.6.0"
  }
}
